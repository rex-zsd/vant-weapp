<van-picker
  class="van-picker"
  show-toolbar
  value-key="name"
  title="{{ title }}"
  loading="{{ loading }}"
  columns="{{ displayColumns }}"
  item-height="{{ itemHeight }}"
  visible-item-count="{{ visibleItemCount }}"
  bind:change="onChange"
  bind:confirm="onConfirm"
  bind:cancel="onCancel"
/>
